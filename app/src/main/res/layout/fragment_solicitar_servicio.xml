<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SolicitarServicioFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <EditText
            android:id="@+id/editTextTextPersonName2"
            android:layout_width="220dp"
            android:layout_height="38dp"
            android:ems="10"
            android:inputType="textPersonName"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.764"
            app:layout_constraintStart_toEndOf="@+id/lblComentarios"
            app:layout_constraintTop_toBottomOf="@+id/txtCalle"
            app:layout_constraintVertical_bias="0.11" />

        <TextView
            android:id="@+id/lblSolicitarServicio"
            android:layout_width="220dp"
            android:layout_height="21dp"
            android:text="Solicitar"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.083"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.073" />


        <TextView
            android:id="@+id/lblTipoServicio"
            android:layout_width="110dp"
            android:layout_height="38dp"
            android:text="Servicio"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.053"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/lblSolicitarServicio"
            app:layout_constraintVertical_bias="0.045" />

        <Spinner
            android:id="@+id/spnspnSelectServicio"
            android:layout_width="220dp"
            android:layout_height="38dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.753"
            app:layout_constraintStart_toEndOf="@+id/lblTipoServicio"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.145" />

        <TextView
            android:id="@+id/lblUbicacion"
            android:layout_width="110dp"
            android:layout_height="21dp"
            android:text="UbicaciÃ³n"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.045"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/lblTipoServicio"
            app:layout_constraintVertical_bias="0.055" />

        <TextView
            android:id="@+id/lblEntidad"
            android:layout_width="110dp"
            android:layout_height="38dp"
            android:text="Entidad"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.043"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/lblUbicacion"
            app:layout_constraintVertical_bias="0.088" />
        <!--            app:layout_constraintTop_toBottomOf="@+id/txtFecNac"-->


        <Spinner
            android:id="@+id/spnspnSelectEntidad"
            android:layout_width="220dp"
            android:layout_height="38dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.764"
            app:layout_constraintStart_toEndOf="@+id/lblEntidad"
            app:layout_constraintTop_toBottomOf="@+id/spnspnSelectServicio"
            app:layout_constraintVertical_bias="0.173" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="110dp"
            android:layout_height="38dp"
            android:text="TextView"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.043"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/lblEntidad"
            app:layout_constraintVertical_bias="0.104" />

        <Spinner
            android:id="@+id/spnspnSelectMunicipio"
            android:layout_width="220dp"
            android:layout_height="38dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.764"
            app:layout_constraintStart_toEndOf="@+id/textView5"
            app:layout_constraintTop_toBottomOf="@+id/spnspnSelectEntidad"
            app:layout_constraintVertical_bias="0.104" />

        <TextView
            android:id="@+id/lblCalle"
            android:layout_width="110dp"
            android:layout_height="38dp"
            android:text="Calle"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.043"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView5"
            app:layout_constraintVertical_bias="0.088" />

        <EditText
            android:id="@+id/txtCalle"
            android:layout_width="220dp"
            android:layout_height="38dp"
            android:ems="10"
            android:inputType="textPersonName"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.764"
            app:layout_constraintStart_toEndOf="@+id/lblCalle"
            app:layout_constraintTop_toBottomOf="@+id/spnspnSelectMunicipio"
            app:layout_constraintVertical_bias="0.088" />

        <TextView
            android:id="@+id/lblComentarios"
            android:layout_width="110dp"
            android:layout_height="38dp"
            android:text="Comentarios"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.043"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/lblCalle"
            app:layout_constraintVertical_bias="0.107" />

        <Button
            android:id="@+id/btnConfirmar"
            android:layout_width="220dp"
            android:layout_height="wrap_content"
            android:text="Confirmar Solicitud"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.916"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName2"
            app:layout_constraintVertical_bias="0.202" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>